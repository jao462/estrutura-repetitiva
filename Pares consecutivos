#include <iostream>
using namespace std;

int main() {
    int X;

    while (true) {
        cin >> X;

        if (X == 0) {
            break;
        }

        int soma = 0;
        int contador = 0;

        
        int num = (X % 2 == 0) ? X : X + 1;

       
        for (int i = 0; i < 5; i++) {
            soma += num;
            num += 2; 
        }

        cout << soma << endl;
    }

    return 0;
}
