#include <iostream>
#include <iomanip>
using namespace std;

int main() {
    int N;
    cin >> N;

    for (int i = 0; i < N; i++) {
        double A, B, C;
        cin >> A >> B >> C;

        double media = (A*2 + B*3 + C*5) / 10.0;

        cout << fixed << setprecision(1) << media << endl;
    }

    return 0;
}
