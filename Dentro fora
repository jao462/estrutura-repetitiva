#include <iostream>
using namespace std;

int main() {
    int N;
    cin >> N;

    int dentro = 0, fora = 0;

    for (int i = 0; i < N; i++) {
        int X;
        cin >> X;

        if (X >= 10 && X <= 20) {
            dentro++;
        } else {
            fora++;
        }
    }

    cout << dentro << " dentro" << endl;
    cout << fora << " fora" << endl;

    return 0;
}
