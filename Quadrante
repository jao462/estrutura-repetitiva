#include <iostream>
using namespace std;

int main() {
    int X, Y;

    while (true) {
        cin >> X >> Y;

        if (X == 0 || Y == 0) {
            break;
        }

        if (X > 0 && Y > 0) {
            cout << "Q1" << endl;
        } else if (X < 0 && Y > 0) {
            cout << "Q2" << endl;
        } else if (X < 0 && Y < 0) {
            cout << "Q3" << endl;
        } else if (X > 0 && Y < 0) {
            cout << "Q4" << endl;
        }
    }

    return 0;
}
