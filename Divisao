#include <iostream>
#include <iomanip>
using namespace std;

int main() {
    int N;
    cin >> N; 

    for (int i = 0; i < N; i++) {
        double A, B;
        cin >> A >> B;

        if (B == 0) {
            cout << "DIVISAO IMPOSSIVEL" << endl;
        } else {
            cout << fixed << setprecision(1) << A / B << endl;
        }
    }

    return 0;
}
