#include <iostream>
using namespace std;

int main() {
    int senha;

    while (true) {
        cin >> senha;

        if (senha == 2002) {
            cout << "Acesso Permitido" << endl;
            break; a
        } else {
            cout << "Senha Invalida! Tente novamente:" << endl;
        }
    }

    return 0;
}
