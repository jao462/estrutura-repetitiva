#include <iostream>
using namespace std;

int main() {
    double nota1, nota2;

  
    while (true) {
        cin >> nota1;
        if (nota1 >= 0 && nota1 <= 10) {
            break; // nota válida, sai do loop
        } else {
            cout << "nota invalida" << endl;
        }
    }

   
    while (true) {
        cin >> nota2;
        if (nota2 >= 0 && nota2 <= 10) {
            break; // nota válida, sai do loop
        } else {
            cout << "nota invalida" << endl;
        }
    }

    double media = (nota1 + nota2) / 2.0;
    cout.precision(2);
    cout << fixed << "MEDIA = " << media << endl;

    return 0;
}
